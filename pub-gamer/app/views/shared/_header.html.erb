 <div class="top-bar">
  <div class="row small-collapse">
    <div class="small-12 columns">
      <%= link_to image_tag("topbarBannerOrange.png"), method: "get", action: "index", controller: "welcome" %>
    </div>
  </div>

  <div class="row small-collapse align-spaced">
    <div class="small-12 medium-7 columns">
      <ul class="menu horizontal align-self-center">
        <% if logged_in? %>
          <li><%= link_to "Profile", user_path(current_user.id), id: "user" %></li>
        <% end %>
        <li><%= link_to "Venues", venues_path, id: "venues" %></li>
        <li><%= link_to "Events", events_path, id: "events" %></li>
        <li><%= link_to "About" %></li>
        <li><%= link_to "Contact" %></li>
      </ul>
    </div>
  <!-- </div> -->

  <!-- <div class="row small-collapse" "> -->
    <div class="small-12 medium-4 columns" id="topbar-row-three">
      <%= render partial: '/shared/search_form' %>
    </div>
  </div>
</div>