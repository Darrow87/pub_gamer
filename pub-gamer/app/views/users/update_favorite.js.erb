var notice = $target.closest('.index-bottom').find('.index-bottom-notice p')
<% if @user.favorite_added?(@old_fav_num) %>
	var note = "Venue added to favorites"
	$target.replaceWith("<%= j render partial: 'venues/index_remove_favorite', locals: { venue: @venue } %>");
<% else %>
	var note = "Venue removed from favorites"
	$target.replaceWith("<%= j render partial: 'venues/index_add_favorite', locals: { venue: @venue } %>");
<% end %>
notice.text(note);
setTimeout(function(){
	notice.text("")
}, 3000);
