<div id="login-box">
  <%= form_tag new_login_path, id: 'login-form' do %>
  	<h4>Login</h4>
    <%= text_field_tag :user_name, nil, placeholder: "User Name", required: true, title: "User Name" %>
    <%= email_field_tag :email, nil, placeholder: "Email", required: true, title: "Email" %>
    <%= password_field_tag :password, nil, placeholder: "Password", required: true, title: "Password" %>
    <%= submit_tag "Log In", class: "button large", id: "login-button" %>
  <% end %>
</div>