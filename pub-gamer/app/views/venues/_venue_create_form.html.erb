	<!-- <div class="small-12 columns"> -->
		<%= form_for @venue, :html => {:id => "venue-create-form"} do |f| %>
<div class="row">
			<!-- <div class="row"> -->
				<div class="small-6 columns">
	    		<%= f.text_field :name, required: true, placeholder: "Name" %>
	  		</div>
	  		<div class="small-6 columns">
	    		<%= f.text_field :address, required: true, placeholder: "Address" %>
	  		</div>
			</div>
	    <div class="row">
		    <% if @games && !@games.empty? %>
		    	<div class="small-12 columns text-center" id="venue-create-games">
		      	<% @games.each do |game| %>
		      		<div class="venue-create-game", title="games">
		        		<%= check_box_tag "venue[games][]", game.id, false, class: "venue-create-game-button" %><br>
		        		<p class="venue-create-game-label"><%= game.name %></p>
		        	</div>
		      	<% end %>
		  		</div>
		    <% end %>
	  	</div>
	  	<div class="row">
	  		<div class="small-7 columns">
	  		<!-- <div class="small-5 columns"> -->
	    		<%= f.text_field "game", placeholder: "Add Another Game", id: "other-text-field" %>
	  		<!-- </div> -->
	  		<!-- <div class="small-2 columns"> -->
	  		<%= f.button "Add Game!", id: "add-new-game" %>
	  		</div>
	  		<div class="small-5 columns">
	    		<%= f.text_field :neighborhood, required: true, placeholder: "neighborhood" %>
	  		</div>
	  	</div>

	  	<%= f.text_field :place, type: 'hidden' %>
	  	<div class="row align-spaced">
	  		<div class="small-4 columns">
	    		<%= f.submit "Add venue!", class: "button", id: "venue-create-submit-button" %>
	    	</div>
	  	</div>
  	<% end %>
  <!-- </div> -->
<!-- </div> -->