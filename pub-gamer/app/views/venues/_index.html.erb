
<div class="row" >
	<div class="hide-for-small-only medium-1 columns">
	</div>
	<div class="small-10 medium-4 columns">
			<%=link_to("Add New Venue!", new_venue_path, class: "button", id: "venue-create-link") %>
	</div>
	<div class="hide-for-small-only medium-7 columns">
	</div>

</div>
<div class="row">
	<div class="hide-for-small-only medium-1 columns">
	</div>
	<div class="small-12 medium-9 columns index-main">
		<% if @venues && @venues.length > 0 %>
			<% @venues.each do |venue| %>
  			<%= render partial: 'index_venue_display', locals: { venue: venue } %>
			<% end %>
  	<% else %>
  		<p>No Venues</p>
		<% end %>
  		<%= will_paginate @venues %>
	</div	>
</div>