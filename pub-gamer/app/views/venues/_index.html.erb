
<div class="row align-spaced" >
	<div class="small-10 medium-6 columns">
			<%=link_to("Add New Venue!", new_venue_path, class: "button", id: "venue-create-link") %>
	</div>
</div>

<div class="row align-center">
	<div class="columns notice">
		<% if flash[:notice] %>
			<p><%= flash[:notice] %></p>
		<% end %>
	</div>
</div>

<div class="row small-collapse expanded small-up-1 medium-up-2 large-up-3 align-spaced index-main">
	<!-- <div class="small-12 medium-12 large-10 columns index-main"> -->
	<% if @venues && @venues.length > 0 %>
		<% @venues.each do |venue| %>
 			<%= render partial: 'index_venue_display', locals: { venue: venue } %>
		<% end %>
 	<% else %>
 		<p>No Venues</p>
	<% end %>
</div>
<div class="row columns">
	<%= will_paginate @venues %>
</div>